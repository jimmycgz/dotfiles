alias gh='history|grep'

# search aliases and commands
alias gk='cat_k8s|grep'

function find_k8s_alias () {
    echo " "
    if [ -e $HOME/.bash_kubectl_aliases ]; then
        cat $HOME/.bash_kubectl_aliases | grep $1
    elif [ -e $HOME/.bashrc_kubectl_aliases ]; then
        cat $HOME/.bashrc_kubectl_aliases | grep $1
    fi
    echo " "
}
alias gk='find_k8s_alias'

